<div class="container text-center">
  <h1>All Time High Scores!</h1>
  <table class="table">
    <thead>
      <tr>
        <th scope="col">Name</th>
        <th scope="col">Wins</th>
        <th scope="col">Losses</th>
        <th scope="col">Ties</th>
      </tr>
    </thead>
    <tbody>
      <% @users.each.with_index do |user,index| %>
        <%= user.username %>
    <% end %>
    </tbody>
  </table>
</div>
